<maze301_npc73 id="39" originalRow="39" action="设置角色" data="@char_1" param="pos=中"/>
<maze301_npc73 id="40" originalRow="40" action="角色台词" data="@char_1" param="谁要理他们啊！有宝藏的话当然是自己独吞啊。我已经得到了最可靠的情报！" param3="name=来探险的人"/>
<maze301_npc73 id="41" originalRow="41" action="角色台词" data="@char_1" param="巨龙宝藏就在下一层，而我一定会是三个人里最先去到下一层的，" param3="name=来探险的人"/>
<maze301_npc73 id="42" originalRow="42" action="角色台词" data="@char_1" param="啊哈哈哈哈哈哈！至于他们的死活我才不管呢！" param3="name=来探险的人"/>

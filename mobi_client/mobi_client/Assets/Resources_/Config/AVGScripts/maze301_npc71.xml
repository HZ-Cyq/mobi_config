<maze301_npc71 id="29" originalRow="29" action="设置镜号" data="avg_basic_story_new" param="no_postprocessing"/>
<maze301_npc71 id="30" originalRow="30"/>
<maze301_npc71 id="31" originalRow="31" action="角色台词" data="wo" param="谁要理他们啊！有宝藏的话当然是自己独吞啊。我已经得到了最可靠的情报！" param3="name=自私的人"/>
<maze301_npc71 id="32" originalRow="32" action="角色台词" data="wo" param="巨龙宝藏就在下一层，而我一定会是三个人里最先去到下一层的，" param3="name=自私的人"/>
<maze301_npc71 id="33" originalRow="33" action="角色台词" data="wo" param="啊哈哈哈哈哈哈！至于他们的死活我才不管呢！" param3="name=自私的人"/>

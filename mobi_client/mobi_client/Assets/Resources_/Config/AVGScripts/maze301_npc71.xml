<maze301_npc71 id="23" originalRow="23" action="设置镜号" data="avg_basic_story_new" param="no_postprocessing"/>
<maze301_npc71 id="24" originalRow="24"/>
<maze301_npc71 id="25" originalRow="25" action="角色台词" data="wo" param="啊对对对，我是和同伴来找宝藏的。那种事情，再说啦，让我再休息一会，再一小会儿就好了。不要打扰我。" param3="name=爱偷懒的人"/>
